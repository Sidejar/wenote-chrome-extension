{
	"extends": ["plugin:@typescript-eslint/recommended", "prettier"],
	"plugins": ["react", "@typescript-eslint", "react-hooks", "prettier"],
	"parser": "@typescript-eslint/parser",
	"root": true,
	"rules": {
		"react-hooks/exhaustive-deps": "warn",
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"trailingComma": "all",
				"endOfLine": "auto",
				"semi": false
			}
		]
	}
}